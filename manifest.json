{
	"name": "RAC Submit",
	"description": "RAC Submit",
	"version": "1.4.1",
	"permissions": [
		"http://s.weibo.com/*",
		"http://weibo.com/*",
		"http://www.weibo.com/*",
		"https://racsubmit.nxmix.com/*"
	],
	"icons" : {
	  	"16" : "icon-16.png",
	  	"48" : "icon-48.png",
	  	"128" : "icon-128.png"
	},	
	"content_scripts": [{
		"matches": [
			"http://s.weibo.com/*",
			"http://weibo.com/*",
			"http://www.weibo.com/*"
		],
		"js": ["j.js", "myscript.js"],
		"css": ["mystyle.css"],
		"run_at": "document_end"
	}],
	"web_accessible_resources": [
		"*.png"
	],
	"manifest_version": 2
}